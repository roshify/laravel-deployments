{
  "name": "roshify/laravel-deployments",
  "description": "A simple, migration-like deployment script manager for Laravel applications.",
  "type": "library",
  "keywords": [
    "laravel",
    "deployment",
    "migrations",
    "scripts"
  ],
  "license": "MIT",
  "require": {
    "php": "^8.1",
    "illuminate/support": "^10.0|^11.0",
    "illuminate/console": "^10.0|^11.0",
    "illuminate/database": "^10.0|^11.0",
    "illuminate/filesystem": "^10.0|^11.0",
    "illuminate/log": "^10.0|^11.0"
  },
  "autoload": {
    "psr-4": {
      "Roshify\\LaravelDeployments\\": "src/"
    }
  },
  "authors": [
    {
      "name": "Roshan Poojary",
      "email": "poojariroshan4@gmail.com"
    }
  ],
  "extra": {
    "laravel": {
      "providers": [
        "Roshify\\LaravelDeployments\\DeploymentServiceProvider"
      ]
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
